{
  "name": "@author.io/metadoc",
  "version": "0.1.2",
  "description": "Generate JSON documentation of OOP-based JavaScript libraries.",
  "bin": {
    "metadoc": "cli.js"
  },
  "main": "generator.js",
  "scripts": {
    "test": "tap -R spec test/*.js && standard --verbose | snazzy",
    "test:debug": "tap -R spec test/*.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "oop",
    "js",
    "docs",
    "metadata",
    "jsdoc"
  ],
  "author": "Corey Butler",
  "license": "BSD 3-Clause",
  "dependencies": {
    "cherow": "1.6.9",
    "comment-parser": "0.5.4",
    "productionline": "1.3.12",
    "traverse": "0.6.6"
  },
  "devDependencies": {
    "snazzy": "8.0.0",
    "standard": "12.0.1",
    "tap": "^14.2.0"
  },
  "standard": {
    "ignore": [
      "_*",
      "_**/*",
      "test/**/*",
      "node_modules",
      "build.js"
    ],
    "globals": [
      "window",
      "global",
      "MutationObserver",
      "DOMParser",
      "XMLHttpRequest",
      "FormData",
      "btoa",
      "localStorage",
      "fetch",
      "Element",
      "HTMLElement",
      "NodeFilter",
      "BUS",
      "DOC",
      "LAST_ENTITY"
    ]
  }
}
